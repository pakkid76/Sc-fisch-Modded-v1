--// Set the default theme
Window:SetTheme(Themes.Dark)

--// Sections
Window:AddTabSection({
	Name = "Menu Fishing",
	Order = 1,
})

Window:AddTabSection({
	Name = "Settings",
	Order = 2,
})

--// Tab [MAIN]

local Main = Window:AddTab({
	Title = "MENU",
	Section = "Main",
	Icon = "rbxassetid://11963373994"
})

Window:AddSection({ Name = "INFORMATIONðŸ‘‡", Tab = Main }) 


Window:AddParagraph({
	Title = "INFO",
	Description = "FISCH MODDED V2 TEST GUI.",
	Tab = Main
}) 

Window:AddSection({ Name = "BYPASS", Tab = Main }) 

Window:AddButton({
	Title = "BYPASS ANTI CHEAT",
	Description = "Click",
	Tab = Main,
	Callback = function() 
		Window:Notify({
			Title = "BYPASS SUCCESSFUL ",
			Description = "FULLY ANTI CHEAT", 
			Duration = 10
		})
	end,
}) 

Window:AddSection({ Name = "MENU REEL", Tab = Main }) 


Window:AddButton({
	Title = "AUTO REEL NOT SAFE",
	Description = "ON",
	Tab = Main,
	Callback = function() 
	_G.auto = true


while _G.auto do
local args = {
    [1] = 100,
    [2] = true
}
game:GetService("ReplicatedStorage"):WaitForChild("events"):WaitForChild("reelfinished"):FireServer(unpack(args))
wait(0)
end
		Window:Notify({
			Title = "SUCCES",
			Description = "", 
			Duration = 10
		})
	end,
}) 


